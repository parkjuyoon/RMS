package drl;
import java.util.Map;
import static main.func.DaySinceLastEvent.run;

rule "CheckIsEmpty1"
	no-loop true
	lock-on-active true
	salience 1
	when
		$map : Map(
			this["INSUR_PROD_SBSC_YN"]=="Y" &&
			this["IPTV_SBSC_YN"]<"50" ||
		    eval(run(new Object[] {"wa01", 30}) == false)
		)
	then
	    $map.put("function_result1", "결과값");
end

