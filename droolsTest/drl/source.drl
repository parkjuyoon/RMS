package drl;
import java.util.Map;
import static com.secbro.drools.utils.DroolsStringUtils.*;

rule "CheckIsEmpty"
	no-loop true
	lock-on-active true
	salience 1
	when
		$map : Map(
			this["INSUR_PROD_SBSC_YN"]=="Y" &&
			this["IPTV_SBSC_YN"]<"50" ||
		    eval(main("#", 12) == false)
		)
	then
	    $map.put("function_result", "결과값");
	end